<% if(renderCarrito.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: green"></tr>
            <th>id</th>
            <th>fechayhora</th>
            <th>productos</th>
            <th></th>

                <% renderCarrito.forEach(function(carrito){%>
            <tr>

            <td><%=carrito.id%></td>
            <td><%=carrito.date%></td>
            <% carrito.productos.forEach(function(producto){%>
            <td><%=producto.title%></td>
            <td><%=producto.thumbnail%></td>
            <% });%>


            </tr>
        <% });%>
    </table>
    </div>
        <% } else { %>
            <h2>no hay datos</h2>
        <% } %>