<% if(renderProductos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: green"></tr>
            <th>Title</th>
            <th>price</th>
            <th>thumbnail</th>
            <th>id</th>

                <% renderProductos.forEach(function(producto){%>
            <tr>

            <td><%=producto.title%></td>
            <td><%=producto.price%></td>
            <td><%=producto.thumbnail%></td>
            <td><%=producto.id%></td>
        </tr>
        <% });%>
    </table>
    </div>
        <% } else { %>
            <h2>no hay datos</h2>
        <% } %>